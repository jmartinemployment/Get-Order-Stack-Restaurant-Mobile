{
  "name": "get-order-stack-restaurant-mobile",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "kds": "npm run start --workspace=apps/kds",
    "pos": "npm run start --workspace=apps/pos",
