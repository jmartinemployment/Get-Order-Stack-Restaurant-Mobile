    "pos:android": "npm run android --workspace=apps/pos",
    "build:kds": "npm run build --workspace=apps/kds",
    "build:pos": "npm run build --workspace=apps/pos"
  },
  "devDependencies": {
    "typescript": "~5.9.2"
  },
  "overrides": {
    "glob": "^10.3.10",
    "inflight": "npm:lru-cache@^10.2.0"
  }
}
